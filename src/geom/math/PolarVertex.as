package geom.math {
	public class PolarVertex {
		private var r:Number;
		private var xRotation:Number;
		private var yRotation:Number;
		private var zRotation:Number;
		
		public function PolarVertex(r:Number, xRotation:Number, yRotation:Number, zRotation:Number) {
			this.r = r;
			this.xRotation = xRotation;
			this.yRotation = yRotation;
			this.zRotation = zRotation;
		}
	}
}